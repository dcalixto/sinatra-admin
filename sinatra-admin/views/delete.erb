<html>
<head>
<title>Sinatra Admin</title>
 	<link href="css/bootstrap.css" rel="stylesheet">
	<link href="css/bootstrap-responsive.css" rel="stylesheet">
	<link href="css/docs.css" rel="stylesheet">
	<link href="css/style.css" rel="stylesheet">
	

<script src="js/jquery-1.7.1.min.js" type="text/javascript"></script>
<script type="text/javascript" src="js/jquery.fancybox-1.3.4.pack.js"></script>

<script type="text/javascript">
	$('document').ready(function(){
	
	
})
	
</script>

</head>

<body>
 
<div id="main">
  <!--------------- HEADER PART STARTS HERE  --------------------------------->	
	<%= erb :header , :layout =>false %>
 
  <!---------  HEADER PART ENDS HERE  	-------------------------------------->
 
  		
  <!---------  	Content  -------------------->

	<div class="bcontent">
		<div class="container-fluid">
		<div class="row-fluid">
		
		<!-- sidebar -->
		<div class="span3">
		<%= erb :sidebar_tablist, :layout => false %>
		</div><!--span3 -->
	
		<!--right part -->
		<div class="span9">
		<div class="row-fluid">
		<div class="content">
		<div class="page-header"><h1>Delete <%=@name %>&nbsp;<%= @item %></h1>
		</div>
		<ul class="breadcrumb">
		<li class=""><a class="" href="">Dashboard</a></li>
		<span class="divider">/</span>
		<li class=""><a class="" href="/tables?main=active&tab_name=<%=@tab_name%>&name=tabbed_list" style="text-transform:capitalize;"><%=@name %></a></li>
		<span class="divider">/</span>
		<li class="active"><a class="" href="" style="text-transform:capitalize;"><%= @item %></a></li>
		</ul>

		<ul class="nav nav-tabs">

			
			<li class="icon" rel="" title="">
	<a id="show1" href="/show?id=<%=@last_links_id%>&name=<%=@name%>&tab_name=<%=@tab_name%>&item=<%=@item%>">
	<i class="icon-info-sign"></i><span>Show</span></a>
			</li>
			<li class="icon edit_member_link" rel="" title="">
	 <a class="" href="/edit?id=<%=@last_links_id%>&name=<%=@name%>&tab_name=<%=@tab_name%>&item=<%=@item%>">
	<i class="icon-pencil"></i><span>Edit</span>
			</a>
			</li>

			<li class="icon delete_member_link active" rel="" title="">
	 <a class="" href="/delete?id=<%=@last_links_id%>&name=<%=@name%>&tab_name=<%=@tab_name%>&item=<%=@item%>">
	<i class="icon-remove"></i><span>Delete</span>
			</a>
			</li>

			<li class="icon history_show_member_link " rel="" title="">
	<a class="" href="/history?id=<%=@last_links_id%>&name=<%=@name%>&item=<%=@item%>&tab_name=<%=@tab_name%>">
	<i class="icon-book"></i><span>History</span>
			</a>
			</li>

		</ul>

			<h4>
			Are you sure you want to delete this <span style="text-transform:capitalize"><%=@name%> </span>“
			<strong><%= @item %></strong>”
			<span></span>
			? The following related items may be deleted or orphaned:
			</h4>
			<ul>
			<li style="display:block; margin-top:10px">
			<span class="label" style="text-transform:capitalize"><%=@name%></span>
			<a href="#" style="text-transform:capitalize"><%=@name%> <%=@last_links_id %></a>
			</li>
			</ul>
			<form  method="post" action="/deleteitems">
			<input type="hidden"  name="id"  value="<%=@last_links_id %>">
			<input type="hidden"  name="name"  value="<%=@name %>">
			<input type="hidden"  name="item"  value="<%= @item %>">
			<input type="hidden" name="tab_name" value="<%=@tab_name%>" >
			<div style="margin:0;padding:0;display:inline">
			<div class="form-actions">
			<button class="btn btn-danger" type="submit">
			<i class="icon-white icon-ok"></i>
			Yes, I'm sure
			</button>
			<button class="btn" type="" name="_continue">
			<i class="icon-remove"></i>
			Cancel
			</button>
			</div>
			</form>			

			</div><!--span9-->
		</div>		

 <!---------  Content PART ENDS HERE  	-------------------------------------->  
  <!-----------  FANCY BOX CONTENT  ------------------------------------->


	</div><!--main-->
	</div><!--container fluid -->
	</div>	
	<!-- Bootstrap js files -->
	    <script src="js/jquery.js"></script>
	    <script src="js/google-code-prettify/prettify.js"></script>
	    <script src="js/bootstrap-transition.js"></script>
	    <script src="js/bootstrap-alert.js"></script>
	    <script src="js/bootstrap-modal.js"></script>
	    <script src="js/bootstrap-dropdown.js"></script>
	    <script src="js/bootstrap-scrollspy.js"></script>
	    <script src="js/bootstrap-tab.js"></script>
	    <script src="js/bootstrap-tooltip.js"></script>
	    <script src="js/bootstrap-popover.js"></script>
	    <script src="js/bootstrap-button.js"></script>
	    <script src="js/bootstrap-collapse.js"></script>
	    <script src="js/bootstrap-carousel.js"></script>
	    <script src="js/bootstrap-typeahead.js"></script>
	    <script src="js/application.js"></script>

</body>
</html>
