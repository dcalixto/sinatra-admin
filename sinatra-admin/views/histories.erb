	<table id="history" class="table table-striped table-condensed">
	<thead>
	<tr><th class="header">Date/Time</th>
	<th class="header">User</th>
	<th class="header">Item</th>
	<th class="header">Message</th>
	</tr>
	</thead>
	<tbody>
	<% if @history %>
	<% @rev=@history.reverse %>	
	<% @rev.each do |history|%>
	<tr>	
	<td><%=history.created_at.strftime("%d-%m-%Y")%></td><td><%=history.username%></td><td><%=history.id%></td>
	<% if history.item==1%>	
	<td>created &nbsp;<%=history.table_name%>&nbsp;<%=history.message%></td>
	<%elsif history.item==2 %>	
	<td>updated &nbsp;<%=history.table_name%>&nbsp;<%=history.message%></td>
	<%elsif history.item==3%>
	<td>deleted &nbsp;<%=history.table_name%>&nbsp;<%=history.message%></td>
	</tr>	
	<%end%>	
	<%end%>
	
	<%end%>
	</tbody>
	</table>
